---
import logociencias from '../images/Cs_hcga.png';
import logofisica from '../images/f√≠sica-horizontal.png';
---
<div class="top ">
    <image id="logociencias" class="logociencias" src={logociencias.src} alt="Logo Ciencias ULA" fetchpriority="high"></image>
    <image id="logofisica" class="logofisica" src={logofisica.src} alt="Logo Ciencias ULA" fetchpriority="high"></image>
</div>

<style>
    .top{
        height: 15%;
        background: #d5b076;
        width: 100%;
    }
    .logociencias{
        width: 30%;
        margin-top: 20px;
        margin-left: 20px;
        filter: drop-shadow(0px 1px 0 #202020);
    }
    .logofisica{
        width: 13%;
        margin-right: 20px;
        float: right;
        filter: drop-shadow(2px 3px 0 #000);
    }
</style>
